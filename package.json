    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "dotenv-cli": "7.4.2",
    "eslint": "9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.35.0",
